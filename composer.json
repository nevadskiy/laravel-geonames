{
  "name": "nevadskiy/laravel-geonames-module",
  "description": "The package allows to integrate geonames database with a Laravel application.",
  "license": "MIT",
  "keywords": ["geonames", "geo", "database", "countries", "cities", "laravel"],
  "homepage": "https://github.com/nevadskiy/laravel-geonames-module",
  "authors": [
    {
      "name": "Nevadskiy",
      "email": "nevadskiy@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^5.0",
    "mockery/mockery": "^1.2",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "Nevadskiy\\Geonames\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nevadskiy\\Geonames\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nevadskiy\\Geonames\\GeonamesServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "fix": "php-cs-fixer fix"
  }
}
